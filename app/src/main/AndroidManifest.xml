<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="studio.lyoko.coinpoly">

    <application
        android:name=".dashboardpage.GameData"

        android:allowBackup="true"
        android:icon="@drawable/logo_mark"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_mark"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".mainpages.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainpages.MainActivity" />
    </application>



</manifest>